{
    "name": "renedekat/laravel-view-finder-creater",
    "type": "library",
    "description": "A Laravel view finder replacement that creates missing views with a template layout.",
    "keywords": [
        "renedekat",
        "laravel-view-finder-creater"
    ],
    "homepage": "https://github.com/renedekat/laravel-view-finder-creater",
    "license": "MIT",
    "authors": [
        {
            "name": "Rene de Kat",
            "email": "renedekat@9lives-development.com",
            "homepage": "https://9lives-development.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.0"
    },
    "autoload": {
        "psr-4": {
            "ReneDeKat\\LaravelViewFileFinder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReneDeKat\\LaravelViewFileFinder\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
